  langid = {english},
  keywords = {Artigo de revisao,Bases de dados,Metodologia,Revisao sistematica,ZoteroTALASE},
  file = {/Users/omarperezbanos/Library/Mobile Documents/com~apple~CloudDocs/ixmatilistli/Ocaña-Fernández y Fuster-Guillén - 2021 - The bibliographical review as a research methodolo.pdf}
