@article{Estefan2023,
  title = {From {{Inclusive}} to {{Equitable Pedagogy}}: {{How}} to {{Design Course Assignments}} and {{Learning Activities That Address Structural Inequalities}}},
  shorttitle = {From {{Inclusive}} to {{Equitable Pedagogy}}},
  author = {Estefan, Michel and Selbin, Jesse Cordes and Macdonald, Sarah},
  year = {2023},
