@article{Sandu2024,
  title = {The Role and Impact of {{ChatGPT}} in Educational Practices: Insights from an {{Australian}} Higher Education Case Study},
  shorttitle = {The Role and Impact of {{ChatGPT}} in Educational Practices},
  author = {Sandu, Raj and Gide, Ergun and Elkhodr, Mahmoud},
  year = {2024},
  month = jun,
  journal = {Discover Education},
  volume = {3},
  number = {1},
  pages = {71},
