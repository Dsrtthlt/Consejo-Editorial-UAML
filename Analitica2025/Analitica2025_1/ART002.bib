@article{Puga2015,
  title = {{{\textquestiondown}Teor{\'i}a social latinoamericana o desde Am{\'e}rica Latina?: Para una sociolog{\'i}a de la modernidad realmente existente}},
  shorttitle = {{{\textquestiondown}Teor{\'i}a social latinoamericana o desde Am{\'e}rica Latina?}},
  author = {Puga, Ismael},
  year = {2015},
