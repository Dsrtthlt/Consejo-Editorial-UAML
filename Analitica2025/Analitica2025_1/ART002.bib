@article{Rautio2022,
  title = {``{{For}} Whom? {{By}} Whom?'': Critical Perspectives of Participation in Ecological Citizen Science},
  shorttitle = {``{{For}} Whom?},
  author = {Rautio, Pauliina and Tammi, Tuure and Aivelo, Tuomas and Hohti, Riikka and Kervinen, Anttoni and Saari, Maria},
  year = {2022},
  month = sep,
  journal = {Cultural Studies of Science Education},
  volume = {17},
  number = {3},
  pages = {765--793},
  issn = {1871-1502, 1871-1510},
  doi = {10.1007/s11422-021-10099-9},
  urldate = {2024-06-26},
  abstract = {Abstract                            This paper is a search for common ground between two natural scientists, two childhood studies and education scholars and two human--animal studies or critical animal studies scholars all working within a shared citizen science project. The search takes the form of a thematic mapping of existing literature on ecological citizen science, with two questions: ``Participation on whose terms?'', and ``Participation for whose benefit?''. First, we draw from the field of               childhood studies               to show how the concept of participation can be theorized further. Second, we recontextualize ecological citizen science research from a               multispecies               perspective, following the non-anthropocentric turn in human and social sciences which has so far drawn limited attention both in educational research and in citizen science projects. We proceed by critically treading the blurry line between predetermined or science-led participation and emergent or participant-led research, forming a fruitful space for examining and reconceptualizing the prevailing human/nature distinction in science and pedagogy. What we end up proposing is not so much a solution to the issues we have located, but rather an invitation to consider participation as a possibility for engaging with the ongoing tensions regarding the apparatuses of power that guide the research practices, researchers' thinking and ethics. For the democratic ethos of citizen science projects, these observations can result in an ongoing process of asking how would it be possible to make space for various knowledges to be regarded as such: How could different kinds of knowledge co-exist, potentially generating more just worlds?},
  langid = {english},
  keywords = {CienciaAbierta,CitizenScience,DatosAbiertos,EticaOA,OpenData},
  file = {/Users/omarperezbanos/Library/Mobile Documents/com~apple~CloudDocs/ixmatilistli/Rautio et al. - 2022 - “For whom By whom” critical perspectives of par.pdf}
}

