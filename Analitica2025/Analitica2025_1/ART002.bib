  title = {{Ciencias cognitivas y educaci{\'o}n: Una propuesta de di{\'a}logo}},
  shorttitle = {{Ciencias cognitivas y educaci{\'o}n}},
  author = {Fuentes Canosa, Adela and Uma{\~n}a Serrato, Jennifer Paola and Risso Migues, Alicia and Facal Mayo, David},
  year = {2021},
