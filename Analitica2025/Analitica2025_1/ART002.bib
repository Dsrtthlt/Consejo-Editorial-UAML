@article{Osborne1999,
  title = {Do the Social Sciences Create Phenomena?: The Example of Public Opinion Research},
  shorttitle = {Do the Social Sciences Create Phenomena?},
  author = {Osborne, Thomas and Rose, Nikolas},
  year = {1999},
  journal = {The British Journal of Sociology},
  volume = {50},
  number = {3},
  pages = {367--396},
  issn = {1468-4446},
  doi = {10.1111/j.1468-4446.1999.00367.x},
  urldate = {2024-07-10},
  abstract = {This paper is an investigation into the philosophy and the history of the social sciences. Some philosophers of the social sciences have suggested that a key feature of the natural sciences is their capacity to create phenomena, and that the social sciences do not meet this criterion. We suggest, to the contrary, that the social sciences can and do create phenomena, in the sense of new ways of describing and acting that have been used to produce all sorts of effects. Like the natural sciences, the social sciences create their phenomena through the procedures that are established to discover them. But the creation of phenomena is a complex, technically difficult and contested process and its success rare. Historically, this argument is developed through a case-study of the development and evolution of public opinion research in the USA and Britain. We argue that by the 1950s public opinion produced a version of the world that had entered `into the true'. Special attention is given to technical considerations in the development of public opinion research, especially the genealogy of a particular research technology, that of the representative sample. Whilst we are not concerned with demarcation criteria, we argue that there are some important differences between the social and the natural sciences; that the former have a less concentrated `spatial mix' and a slower `tempo of creativity'. None the less, in this particular case, the social sciences have played a key role in the creation of opinioned persons and an opinionated society.},
  langid = {english},
  keywords = {/unread,irrealism,Philosophy of social science,public opinion,research technology,sampling},
  file = {/Users/omarperezbanos/Library/Mobile Documents/com~apple~CloudDocs/ixmatilistli/Osborne y Rose - 1999 - Do the social sciences create phenomena the exam.pdf;/Users/omarperezbanos/Zotero/storage/M8P3JYXT/j.1468-4446.1999.00367.html}
}

