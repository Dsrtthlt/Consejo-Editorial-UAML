@misc{Ahmed2022,
  title = {Feminist {{Ears}}:{{Listening}} to {{Complaint}}, {{Learning}} about {{Violence}}},
  shorttitle = {Feminist {{Ears}}},
  author = {Ahmed, Sara},
  year = {2022},
  month = may,
  address = {Universidad Nacional del Rosario},
  abstract = {Esta conferencia explora por qu{\'e} el proyecto de reparar la violencia, incluida la violencia institucional (la violencia de c{\'o}mo las instituciones responden a la violencia), requiere nuevas formas de escuchar la queja o de adquirir o{\'i}dos feministas. En Vivir una vida feminista (2017), describ{\'i} los o{\'i}dos feministas como la forma en que escuchamos "los sonidos del no, las quejas sobre la violencia, la negativa a re{\'i}rse de los chistes sexistas" como discurso. {\textexclamdown}M{\'a}s recientemente, en Queja! (2021), describo los o{\'i}dos feministas como un m{\'e}todo de investigaci{\'o}n, una forma de recoger y escuchar las quejas, y como una t{\'a}ctica institucional, una forma de presionar contra las propias instituciones en las que se presentan las quejas. En esta conferencia, reflexionar{\'e} sobre lo que he aprendido de la escucha de las denuncias, explorando c{\'o}mo las barreras institucionales que impiden que las denuncias sean escuchadas son las mismas barreras que permiten el acoso y la intimidaci{\'o}n. Adquirir un o{\'i}do feminista es, por tanto, llegar a comprender mejor lo que se necesita para desafiar las culturas institucionales de acoso.},
  keywords = {nosource}
}

