@article{Lannamann2020,
  title = {Unsettling Trauma: From Individual Pathology to Social Pathology},
  shorttitle = {Unsettling Trauma},
  author = {Lannamann, John W. and McNamee, Sheila},
  year = {2020},
  month = aug,
  journal = {Journal of Family Therapy},
  volume = {42},
  number = {3},
  pages = {328--346},
  issn = {0163-4445, 1467-6427},
  doi = {10.1111/1467-6427.12288},
  urldate = {2025-01-15},
  abstract = {Rather than assuming that psychological terms mirror natural categories in the world and that the task of the helping professions is to find ways to discover, diagnose and treat these categories, our goal is to inquire about the status of the categories as an aid to then focusing on therapeutic responses. To that end, we unsettle trauma by exploring its social ontology as a disorder as well as its grammar. Feminist and cultural critiques of PTSD illustrate the limitations of the dominant discourse of trauma, moving trauma from individual pathology to social pathology. We offer two illustrations from the family therapy literature that draw upon polyphony, inner dialogues and the creation of new narratives to expand beyond the pathologising potentials of the discourse of trauma.                                         Practitioner points                                                                        Understanding the concept of trauma as a socially embedded construction is a necessary first step towards recognising its different applications in therapeutic work                                                           The dialogic/constructionist stance taken here assumes that meaning and understanding are shaped by discourse and interaction                                                           The taken-for-granted meanings associated with trauma need to be questioned                                                           Practitioners may then assess when the use of a PTSD diagnosis might import individualising and objectifying assumptions that undermine the goals of systemic family therapy                                                                           ,              抽象                                                            扰动创伤: 从个体病理到社会病理                              我们不假设心理学术语反映了世界上的自然类别, 而助人职业的任务是找到方法去发现、诊断和治疗这些类别。我们的目标是询问这些类别的状况, 以此帮助我们专注于治疗性反应。为此, 我们通过探究创伤作为一种障碍的社会本体论和语法来扰动创伤。对创伤后应激障碍的女性主义和文化批判展现了有关创伤的主流话语的局限性, 并将创伤从个体反常转变为社会反常。我们从家庭治疗文献中提供了两个例证, 它们借鉴了复调、内心对话和新叙事的创造, 在创伤话语的病态化可能性之外进行了拓展。                                         对从业者的启示                                                                        理解创伤这一概念是一种根植于社会的建构是认识它在治疗工作中不同应用的必要的第一步                                                           本文采取的对话/建构主义立场假定意义和理解是由话语和互动形成的                                                           与创伤相关的想当然的含义需要被质疑                                                           之后, 从业者可以评估何时使用创伤后应激障碍诊断可能引入个人化和物化的假设, 从而破坏系统性家庭治疗的目标                                                                           ,              Resumen                                                            Desmontando el Trauma: de la patolog{\'i}a individual a la patolog{\'i}a social                              En lugar de suponer que los t{\'e}rminos psicol{\'o}gicos reflejan categor{\'i}as naturales en el mundo y que la tarea de las profesiones de ayuda es encontrar formas de descubrir, diagnosticar y tratar estas categor{\'i}as, nuestro objetivo es indagar sobre el estado de las categor{\'i}as como una ayuda para luego enfocarnos sobre respuestas terap{\'e}uticas. Con ese fin, desmontamos el trauma al explorar su ontolog{\'i}a social como un trastorno, as{\'i} como su gram{\'a}tica. Las cr{\'i}ticas feministas y culturales del TEPT ilustran las limitaciones del discurso dominante del trauma, trasladando el trauma de la patolog{\'i}a individual a la patolog{\'i}a social. Ofrecemos dos ilustraciones de la literatura sobre terapia familiar que se basan en la polifon{\'i}a, los di{\'a}logos internos y la creaci{\'o}n de nuevas narrativas para expandir m{\'a}s all{\'a} de los potenciales patologizantes del discurso del trauma.                                         Puntos de implicaci{\'o}n pr{\'a}ctica                                                                        Comprender el concepto de trauma como una construcci{\'o}n socialmente integrada es un primer paso necesario para reconocer sus diferentes aplicaciones en el trabajo terap{\'e}utico.                                                           La postura dial{\'o}gica/construccionista adoptada aqu{\'i} supone que el significado y la comprensi{\'o}n est{\'a}n formados por el discurso y la interacci{\'o}n.                                                           Los significados asociados con el trauma deben ser cuestionados y no dar cosas por sentadas.                                                           Los profesionales pueden evaluar cu{\'a}ndo el uso de un diagn{\'o}stico de TEPT podr{\'i}a conllevar suposiciones individualizadoras y cosificadoras que socavan las metas de la terapia familiar sist{\'e}mica},
  langid = {english},
  file = {/Users/omarperezbanos/Library/Mobile Documents/com~apple~CloudDocs/ixmatilistli/Lannamann y McNamee - 2020 - Unsettling trauma from individual pathology to social pathology.pdf}
}

