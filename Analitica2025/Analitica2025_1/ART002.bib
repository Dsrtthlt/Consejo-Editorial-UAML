@article{SabidoRamos2024,
  title = {Por Una Sociolog{\'i}a Pol{\'i}tica de La Percepci{\'o}n. {{Estilos}} de Atenci{\'o}n Feminista En Tiempos de Actitud Blas{\'e}},
