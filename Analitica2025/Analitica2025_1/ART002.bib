  shorttitle = {``{{Cultures}} in Negotiation''},
  author = {Demetriadis, S. and Barbas, A. and Molohides, A. and Palaigeorgiou, G. and Psillos, D. and Vlahavas, I. and Tsoukalas, I. and Pombortsis, A.},
  year = {2003},
