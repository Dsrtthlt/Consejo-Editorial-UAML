@article{Pickersgill2010,
  title = {Psyche, Soma, and Science Studies: {{New}} Directions in the Sociology of Mental Health and Illness},
  shorttitle = {Psyche, Soma, and Science Studies},
  author = {Pickersgill, Martyn D.},
  year = {2010},
