@article{Dommett2023,
  title = {Neuroaesthetics: {{Do}} Neurophysiological Methods Provide a Valid and Reliable Measure of Art Perception},
  shorttitle = {Neuroaesthetics},
  author = {Dommett, Eleanor and Morton, Ruby},
  year = {2023},
  publisher = {Open Science Framework},
  doi = {10.17605/OSF.IO/73RS5},
  urldate = {2023-09-25},
  abstract = {The contemporary field of neuroaesthetics seeks to uncover the neural basis of the perception of artwork (Chatterjee \&amp; Vartanian, 2014). Neuroaesthetics bridges empirical aesthetics, which is traditionally concerned with the statistical properties of images (Taylor et al., 1999) and the psychophysics of visual perception (Fechner, 1871; 1876), with cognitive and affective neuroscience -- fields that utilise neurophysiological techniques to measure or manipulate brain activity involved in cognitive processes (Chatterjee \&amp; Vartanian, 2014). In this review, the perception of artwork is defined as the visual processing of art, as well as its subjective aesthetic judgements (how beautiful we feel an artwork is; Kawabata \&amp; Zeki, 2004). Some theorists argue that art provides a unique view of the human visual system's abilities, with artists harnessing the properties of luminance and colour, motion, and location (processed by the visual `what' and `where' streams, respectively; Chatterjee, 2014), rather than realistically portraying the world in their work (Zeki, 1999). Chatterjee \&amp; Vartanian (2014) propose the judgements and feelings we derive from the perception of artwork emerge from the interplay of three neural systems: sensory-motor, emotion-valuation, and meaning-knowledge. Together, these systems merge top-down information from one's memory and cultural beliefs with bottom-up visual input to form a percept of the piece of artwork before them (Chatterjee \&amp; Vartanian, 2016). The primary neurophysiological methods used in neuroaesthetics research are the functional neuroimaging techniques electroencephalography (EEG) and functional magnetic resonance imaging (fMRI). EEG is a widely used technique that directly measures waveforms of electrical brain activity via electrodes placed on the scalp, which localise and record the neural activity beneath them, albeit with limited spatial resolution but excellent temporal resolution (Jacobsen, 2013; M{\"u}ller-Putz, 2020). Alternatively, fMRI measures haemodynamic changes associated with neural activity by the blood oxygen level dependent (BOLD) response in the brain (Logothetis, 2008). It delivers an indirect measure of brain activity with high spatial, but moderate temporal resolution, correlating blood flow with neural responses during cognitive tasks (Logothetis, 2008). Findings using these techniques indicate that overall, the perception of art appears to engage a distributed network of brain regions (Nadal, 2013; Vartanian \&amp; Skov, 2014) -- namely, the occipital and temporal lobes, typically associated with specialised object recognition -- such as in the fusiform and parahippocampal gyri -- as well as in deeper structures such as the insula and cingulate, commonly associated with emotion (Chatterjee, 2014). Neuroaesthetics has been met with criticism from both scientific and artistic communities (Ball, 2013). fMRI is a contentious technique, with some criticising its modular, `neurophrenological' (Logothetis, 2008) application in cognitive neuroscience and neuro-prefixed fields (Legrenzi \&amp; Umilt{\`a}, 2011). Reverse inferences from fMRI data, or assumptions of brain activity reflecting distinct cognitive processes (Hutzler, 2014), have been drawn from scientists and the public alike in fields such as cognitive neuroscience (Farah, 2014). Similar accusations of hasty conclusions and reverse inferences have been levelled against EEG (Libet et al., 1983; Sinnott-Armstrong \&amp; Simmons, 2021) -- neuroaesthetics is no stranger to such criticisms (Berger, 2022; Kesner et al., 2021). Moreover, the utilisation of neuroimaging techniques within neuroaesthetics has been labelled as reductionist and taking a purely biological stance (Tatar, 2019; Zeki et al., 2020). Despite its small body of proponents claiming neuroaesthetics to be an emerging field for the last 20 years (Chatterjee, 2011; Nadal \&amp; Pearce, 2011), some critics claim it has failed to provide novel insights into the perception of art through its use of neuroimaging techniques (Kesner et al., 2021). Nonetheless, there has been limited in-depth critical consideration of the application of neurophysiological techniques within the field of neuroaesthetics, nor whether they provide valid or reliable measures of the perception of art. Validity is conceptualised by the quality of research and accuracy of the conclusions drawn from it and reliability is defined as the consistency of measures over time (Gravetter \&amp; Forzano, 2018). Within this, the degree of novelty, defined here as original or new research findings, will also be considered. We will critically review studies across the field of neuroaesthetics, considering the role of various top-down and bottom-up influences on the perception of artwork, evaluate the neurophysiological methods used, and provide suggestions for future directions. To deliver a focused discussion on the topic, the artwork in question will primarily concern paintings. We will explore the purported neural basis of the perception of art between realistic and abstract artwork, paintings and photographs, the framing and cultural context of artwork, perception by artists and non-artists, artwork viewed in real life, and the novel application of brain stimulation techniques in neuroaesthetics.},
  collaborator = {{Open Science Framework}},
  copyright = {Creative Commons Attribution 4.0 International},
  keywords = {Arts and Humanities,Biological Psychology,Cognition and Perception,Cognitive Psychology,FOS: Psychology,Life Sciences,Neuroscience and Neurobiology,nosource,Psychology,Social and Behavioral Sciences}
}

@article{Donati2015,
  title = {Manifesto for a Critical Realist Relational Sociology},
  author = {Donati, Pierpaolo},
  year = {2015},
  month = jan,
  journal = {International Review of Sociology},
  volume = {25},
  number = {1},
  pages = {86--109},
  publisher = {Routledge},
  issn = {0390-6701},
  doi = {10.1080/03906701.2014.997967},
  urldate = {2020-09-10},
  abstract = {In recent years, many different versions of relational sociology have appeared. In this paper, I present a critical realist version developed since 1983, which is also called `relational theory of society' (CRRS). It shares with the other relational sociologies the idea of avoiding both methodological individualism and holism. The main differences lie in the way social relations are defined, the kind of reality that is attributed to them, how they configure social formations, and the way in which their changes are conceived (morphogenesis and emergence). In particular, this approach is suitable to understand how the morphogenesis of society comes about through social relations, which are the connectors that mediate between agency and social structure. The generative mechanism that feeds social morphogenesis resides in the dynamic (that is, in their ways of operating) of the social relations networks that alter the social molecule constituting structures already in place. Social morphogenesis is a form of surplus of society with respect to itself. Society increases (or decreases) its potential for surplus depending on processes of valorization (or devalorization) of social relations.},
  keywords = {critical realism,relational sociology,social morphogenesis,social networks,social relations},
  file = {/Users/omarperezbanos/Library/Mobile Documents/com~apple~CloudDocs/ixmatilistli/Donati - 2015 - Manifesto for a critical realist relational sociol.pdf;/Users/omarperezbanos/Zotero/storage/58UR5L6H/03906701.2014.html}
}

