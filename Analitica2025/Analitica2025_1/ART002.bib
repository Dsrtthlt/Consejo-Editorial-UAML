  pages = {63--85},
  publisher = {Annual Reviews},
  issn = {0360-0572, 1545-2115},
  doi = {10.1146/annurev-soc-072320-095202},
  urldate = {2024-10-28},
  abstract = {Paul DiMaggio\&apos;s (1997) Annual Review of Sociology article urged integration of the cognitive and the cultural, triggering a cognitive turn in cultural sociology. Since then, a burgeoning literature in cultural sociology has incorporated ideas from the cognitive sciences---cognitive anthropology, cognitive psychology, linguistics, neuroscience and philosophy---significantly reshaping sociologists' approach to culture, both theoretically and methodologically. This article reviews work published since DiMaggio\&apos;s agenda-setting piece---research that builds on cross-disciplinary links between cultural sociology and the cognitive sciences. These works present new ideas on the acquisition, storage, and retrieval of culture, on how forms of personal culture interact, on how culture becomes shared, and on how social interaction and cultural environments inform cognitive processes. Within our discussion, we point to research questions that remain unsettled. We then conclude with issues for future research in culture and cognition that can enrich sociological analysis about action more generally.},
  langid = {english},
  keywords = {Cognition,CognitiveTurn,CulturalSociology,Culture},
  file = {/Users/omarperezbanos/Library/Mobile Documents/com~apple~CloudDocs/ixmatilistli/Cerulo et al. - 2021 - Rethinking Culture and Cognition.pdf;/Users/omarperezbanos/Zotero/storage/YKVMQEUD/annurev-soc-072320-095202.html}
}

