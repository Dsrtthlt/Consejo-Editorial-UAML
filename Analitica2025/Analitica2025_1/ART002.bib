@book{Hammersley2018,
  title = {Garfinkel and {{Goffman}} via {{Simmel}}: {{Parallels}} and Divergences},
  shorttitle = {Garfinkel and {{Goffman}} via {{Simmel}}},
  author = {Hammersley, Martyn},
  year = {2018},
  month = jul,
  journal = {The radicalism of ethnomethodology},
  edition = {1a},
  pages = {46--67},
  publisher = {Manchester University Press},
  address = {Manchester},
  urldate = {2022-03-20},
  abstract = {This chapter compares the orientations of Garfinkel and Goffman. Their work is often regarded as similar, being concerned with the study of mundane patterns of social interaction. However, ethnomethodologists usually insist that there are fundamental differences between them. Their orientations are examined via a comparison with the work of a third sociologist, Georg Simmel, who was an important influence upon Goffman. While Garfinkel does not seem to have drawn on Simmel's work, there are interesting parallels: in particular, they share a concern with the constitutive role that social interaction plays in social life. It is argued that, despite similarities between the orientations of Garfinkel and Goffman, the differences are more significant. For Goffman, the aim is to generate conceptual frameworks that illuminate everyday behavior, whereas ethnomethodologists resist the bringing in of new concepts, being concerned instead with explicating the processes by which social phenomena are produced in their own terms. Other differences relate to what is taken to be the context of social interaction, with Goffman treating the interaction order as mediating the effects of outside factors, whereas ethnomethodologists insist that the context of any process of social interaction can only be what is constituted as context within it.},
  chapter = {The radicalism of ethnomethodology},
  isbn = {978-1-5261-2463-0},
  langid = {american},
  file = {/Users/omarperezbanos/Library/Mobile Documents/com~apple~CloudDocs/ixmatilistli/Hammersley - 2018 - Garfinkel and Goffman via Simmel Parallels and di 2.pdf;/Users/omarperezbanos/Zotero/storage/ERZVKU3A/9781526124630.00006.html}
}

@incollection{Hammersley2022,
  title = {Sociology as a {{Profession}} in a {{Post-Truth World}}},
  booktitle = {The {{Future}} of {{Sociology}}},
  author = {Hammersley, Martyn},
  year = {2022},
  publisher = {Routledge},
  abstract = {Today, the idea that sociology should be professional has lost favor in many quarters. Instead, it is widely assumed that the discipline ought to be, or intrinsically is, ``oppositional,'' ``critical,'' or ``public'' in orientation. Thus, while Michael Burawoy, in his influential presidential address to the American Sociological Association in 2004, recognized professional sociology as one of four legitimate varieties, his primary emphasis was on public sociology, and this was very much in the spirit of the times. Furthermore, over the second half of the 20th century, sociologists studying the professions became increasingly critical of the very notion of professionalism, often coming to view it as little more than an ideological disguise for the exercise of power. This, too, meant that the idea of ``sociology as a profession'' came to have little appeal. It should be remembered, however, that one of the founding documents of modern sociology treated social science as a vocation or profession.},
  isbn = {978-1-003-19351-7},
